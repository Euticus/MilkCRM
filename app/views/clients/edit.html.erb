<h1>Create a New Client</h1>
<%= form_for @client do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name%><br />

  <%= f.label :phone %>:
  <%= f.text_field :phone %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.submit %>
<% end %>

<%= link_to "back", home_path %>