<h1>Create a New Productt</h1>
<%= form_for @product do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name%><br />

  <%= f.label :location %>:
  <%= f.text_field :name%><br />


  <%= f.label :type %>:
  <%= f.text_field :type %><br />

  <%= f.label :status %>:
  <%= f.text_field :type %><br />

  <%= f.label :price %>:
  <%= f.text_field :price %><br />

  <%= f.label :address %>:
  <%= f.text_field :address %><br />

  <%= f.submit %>
<% end %>
