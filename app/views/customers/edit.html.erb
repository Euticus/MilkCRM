<h1>Create a customer</h1>
<%= form_for @customer do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name%><br />

  <%= f.label :phone %>:
  <%= f.text_field :phone %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :budget %>:
  <%= f.text_field :budget %><br />

  <%= f.submit %>
<% end %>